{
  "version": 2,
  "builds": [
    {
      "src": "web_app/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "web_app/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "web_app/main.py"
    }
  ],
  "functions": {
    "web_app/main.py": {
      "includeFiles": "web_app/**"
    }
  }
}

